{
  "manifest_version": 2,
  "name": "annoyance hider",
  "version": "0.0.2",
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["src/facebook.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://www.quora.com/*"],
      "js": ["src/quora.js"],
      "run_at": "document_idle"
    }
  ]
}
